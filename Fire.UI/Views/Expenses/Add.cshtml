@model AllLayoutViewModel
@{
    ViewData["Title"] = "Add";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<div class="row">
    <header class="page-header">
    </header>

    <div class="col-lg-6 col-md-6">
        <form asp-controller="Expenses" asp-action="Add" name="formadd" method="post" enctype="multipart/form-data">
            <div class="row">
                <div class="col-md-6">
                    <div class="form-group">
                        <label class="control-label"> Açıklama</label>
                        <input id="productName" class="form-control" type="text" asp-for="AddExpenses.Description" required />
                    </div>
                    <div class="form-group">
                        <label class="control-label" for="form-control-3">Gider</label>
                        <input id="unitPrice" class="form-control" asp-for="AddExpenses.ExpensesPrice" required />
                    </div>
                    <div class="form-group row">
                        <div class="col-md-10 offset-md-2">
                            <button type="submit" id="btn1" class=" center-block btn btn-primary">Ekle</button>
                        </div>
                    </div>
                </div>
            </div>
        </form>
    </div>

</div>